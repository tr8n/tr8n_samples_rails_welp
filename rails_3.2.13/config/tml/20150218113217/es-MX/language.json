{
  "id": 90,
  "locale": "es-MX",
  "english_name": "Spanish (Mexico)",
  "native_name": "Español (México)",
  "fallback_language": {
    "id": 79,
    "locale": "es",
    "english_name": "Spanish",
    "native_name": "Español",
    "right_to_left": false,
    "flag_url": "https://d23w63adpjpix6.cloudfront.net/3/80/28/97/54cc192d2452a7d07.png"
  },
  "right_to_left": false,
  "flag_url": "https://d23w63adpjpix6.cloudfront.net/96/76/92/89/98f9bdef92dd735f7.png",
  "contexts": {
    "date": {
      "rules": {
        "future": {
          "description": "{token} is in the past",
          "conditions": "(< @date (today))"
        },
        "past": {
          "description": "{token} is in the future",
          "conditions": "(> @date (today))"
        },
        "present": {
          "description": "{token} is in the present",
          "conditions": "(= @date (today))"
        }
      },
      "keys": [
        "past",
        "present",
        "future"
      ],
      "default_key": "present",
      "token_expression": "/.*(date|time)(\\d)*$/",
      "variables": [
        "@date"
      ],
      "token_mapping": [
        "unsupported",
        "unsupported",
        {
          "past": "{$0}",
          "present": "{$1}",
          "future": "{$2}"
        }
      ]
    },
    "number": {
      "rules": {
        "one": {
          "description": "{n} is 1",
          "examples": "1",
          "conditions": "(= @n 1)"
        },
        "other": {
          "examples": "0, 2-999; 1.2, 2.07..."
        }
      },
      "keys": [
        "one",
        "other"
      ],
      "default_key": "other",
      "token_expression": "/.*(count|num|minutes|seconds|hours|sum|total)(\\d)*$/",
      "variables": [
        "@n"
      ],
      "token_mapping": [
        "unsupported",
        {
          "one": "{$0}",
          "other": "{$1}"
        }
      ]
    },
    "list": {
      "rules": {
        "one": {
          "description": "{token} contains 1 element",
          "conditions": "(= 1 @count)"
        },
        "other": {
          "description": "{token} contains at least 2 elements"
        }
      },
      "keys": [
        "one",
        "other"
      ],
      "default_key": "other",
      "token_expression": "/.*(items|list)(\\d)*$/",
      "variables": [
        "@count"
      ],
      "token_mapping": [
        "unsupported",
        {
          "one": "{$0}",
          "other": "{$1}"
        }
      ]
    },
    "genders": {
      "rules": {
        "female": {
          "description": "{token} contains 1 female",
          "conditions": "(&& (= 1 (count @genders)) (all @genders 'female'))"
        },
        "male": {
          "description": "{token} contains 1 male",
          "conditions": "(&& (= 1 (count @genders)) (all @genders 'male'))"
        },
        "other": {
          "description": "{token} contains at least 2 people"
        },
        "unknown": {
          "description": "{token} contains 1 person with unknown gender",
          "conditions": "(&& (= 1 (count @genders)) (all @genders 'unknown'))"
        }
      },
      "keys": [
        "male",
        "female",
        "unknown",
        "other"
      ],
      "default_key": "other",
      "token_expression": "/.*(users|profiles|actors|targets)(\\d)*$/",
      "variables": [
        "@genders"
      ],
      "token_mapping": [
        "unsupported",
        "unsupported",
        "unsupported",
        {
          "male": "{$0}",
          "female": "{$1}",
          "unknown": "{$2}",
          "other": "{$3}"
        }
      ]
    },
    "gender": {
      "rules": {
        "female": {
          "description": "{token} is a female",
          "conditions": "(= 'female' @gender)"
        },
        "male": {
          "description": "{token} is a male",
          "conditions": "(= 'male' @gender)"
        },
        "other": {
          "description": "{token}'s gender is unknown"
        }
      },
      "keys": [
        "male",
        "female",
        "unknown"
      ],
      "default_key": "other",
      "token_expression": "/.*(user|translator|profile|actor|target)(\\d)*$/",
      "variables": [
        "@gender"
      ],
      "token_mapping": [
        {
          "other": "{$0}"
        },
        {
          "male": "{$0}",
          "female": "{$1}",
          "other": "{$0}/{$1}"
        },
        {
          "male": "{$0}",
          "female": "{$1}",
          "other": "{$2}"
        }
      ]
    }
  },
  "cases": {
  }
}